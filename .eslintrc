{
  "rules": {
    "camelcase": 0,
    "consistent-return": 0,
    "curly": [2, "all"],
    "dot-notation": 0,
    "comma-dangle": 2,
    "eol-last": 0,
    "eqeqeq": [2, "allow-null"],
    "handle-callback-err": 0,
    "indent": [2, "tab"],
    "object-shorthand": 0,
    "one-var": 0,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "strict": [2, "never"],
    "wrap-iife": [2, "inside"],
    "new-cap": [2, {"capIsNew": false}],
    "no-alert": 0,
    "no-caller": 2,
    "no-bitwise": 2,
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extra-semi": 2,
    "no-irregular-whitespace": 0,
    "no-multi-spaces": 0,
    "no-new": 2,
    "no-plusplus": 0,
    "no-process-exit": 0,
    "no-redeclare": 2,
    "no-shadow": 0,
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "no-underscore-dangle": 0,
    "no-undef": 2,
    "no-unused-expressions": ["error", { "allowShortCircuit": true, "allowTernary": true }],
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-use-before-define": 0
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "spread": true
  },
  "globals": {
    "expect": true
  }
}
